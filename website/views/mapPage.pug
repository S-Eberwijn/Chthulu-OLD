block dashboardContent
    if(False)
        include ./components/mapUploadModal.pug 
        .dashboardContent(map=false)
            #map-container
                input(type="file" id="fileElem" multiple accept=".png, .jpeg, .jpg, .gpx, .osm, .zip" data-content="ï‚“ Upload file")
    else
        .dashboardContent(map=true)
            #map-container
                div(id="map")

    script
        include ./static/theme.js
        include ./static/mapDashboard.js
        include ./static/mapUpload.js
