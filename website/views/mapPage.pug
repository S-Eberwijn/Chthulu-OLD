    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/leaflet-easybutton@2/src/easy-button.css")
    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/leaflet-popup-sidebar@1.1.0/css/leaflet-sidebar.min.css")
